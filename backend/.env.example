# Example .env file for backend configuration

# Django secret key
SECRET_KEY=your-secret-key

# Django debug mode (set to False in production)
DEBUG=True

# Allowed hosts (comma-separated, e.g., .kavia.ai,localhost,127.0.0.1)
ALLOWED_HOSTS=.kavia.ai,localhost,127.0.0.1,testserver

# Database URL (for prod/postgres, not needed for local sqlite)
# DATABASE_URL=postgres://user:password@host:5432/dbname

# CORS allowed origins (for production, comma-separated URLs)
# CORS_ALLOWED_ORIGINS=https://your.frontend.url

# Example: add more settings as needed
